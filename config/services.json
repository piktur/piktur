{
  "services": {
    "production": {
      "host": "piktur.io",
      "scheme": "https"
    },
    "development": {
      "host": "localhost",
      "scheme": "http"
    },
    "staging": {
      "host": null,
      "scheme": null
    },
    "test": {
      "host": "localhost",
      "scheme": "http"
    },
    "piktur": {
      "namespace": "Piktur"
    },
    "piktur_core": {
      "namespace": "Piktur"
    },
    "piktur_security": {
      "namespace": "Piktur::Security"
    },
    "piktur_sites": {
      "namespace": "Piktur::Sites"
    },
    "piktur_stores": {
      "namespace": "Piktur::Stores"
    },
    "piktur_spec": {
      "namespace": "Piktur::Spec"
    },
    "piktur_admin": {
      "namespace": "Piktur::Admin",
      "uri": {
        "production": "https://admin.piktur.io",
        "development": "http://localhost:3001",
        "staging": null,
        "test": "http://localhost:3001"
      }
    },
    "piktur_api": {
      "namespace": "Piktur::API",
      "uri": {
        "production": "https://api.piktur.io",
        "development": "http://localhost:3000",
        "staging": null,
        "test": "http://localhost:3000"
      }
    },
    "piktur_blog": {
      "namespace": "Piktur::Blog",
      "uri": {
        "production": "https://blog.piktur.io",
        "development": "http://localhost:2368",
        "staging": null,
        "test": "http://localhost:2368"
      }
    },
    "piktur_client": {
      "namespace": "Piktur::Client",
      "uri": {
        "production": "https://client.piktur.io",
        "development": "http://localhost:3002",
        "staging": null,
        "test": "http://localhost:3002"
      }
    },
    "piktur_docs": {
      "namespace": "Piktur::Docs",
      "uri": {
        "production": "https://docs.piktur.io",
        "development": "http://localhost:3003",
        "staging": null,
        "test": "http://localhost:3003"
      }
    }
  }
}
